			</Parameter>
			<Parameter id="SpecificationLimits" implementation="com.dassault_systemes.enovia.performance_spec.admin_rules.MeasurementCharacteristicLimitsConfiguration">
				<Argument id="SpecificationLimitsArgument" type="boolean" input="checkbox" default="Disabled" />
				<Constant id="expression" value="DisableSpecificationLimitsOnMeasurementCharacteristic" />
			</Parameter>
		</Family>
	</Domain>

</OOTBParameterization>
