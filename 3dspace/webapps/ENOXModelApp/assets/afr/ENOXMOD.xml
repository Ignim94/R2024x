<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Template syp:name="ENOMODHeaders" Target="CATCmdHeadersList">
      <CATCmdHeadersList>
        <CATCommandHeader ID="MaturityHdr" ClassName="DS/LifecycleCmd/MaturityCmd" ResourceFile="LifecycleWidget/LifecycleCmd" Available="1"/>
        <CATCommandHeader ID="RevisionHdr" ClassName="DS/LifecycleCmd/ReviseCmd" ResourceFile="LifecycleWidget/LifecycleCmd" Available="1"/>
        <CATCommandHeader ID="NewBranchHdr" ClassName="DS/LifecycleCmd/NewBranchCmd" ResourceFile="LifecycleWidget/LifecycleCmd" Available="1"/>
        <CATCommandHeader ID="CompareHdr" ClassName="DS/CompareCmd/CompareCmd" ResourceFile="CompareCmd/CompareCmd" Available="1" />
      </CATCmdHeadersList>
  </Template>

  <Template syp:name="ENOMODWkbAccess" Target="CATCmdWorkbench">
      <CATCmdWorkbench Name="ENOMODWkb">
      	<CATCmdContainer Name="Lifecycle" RepresentationType="AfrActionBarSection">
      		<CATCmdStarter Name="Revision" Command="RevisionHdr" />
          <CATCmdStarter Name="NewBranch" Command="NewBranchHdr" />
          <CATCmdStarter Name="Compare" Command="CompareHdr" />
       	</CATCmdContainer>
      </CATCmdWorkbench>
  </Template>

</Styles>
